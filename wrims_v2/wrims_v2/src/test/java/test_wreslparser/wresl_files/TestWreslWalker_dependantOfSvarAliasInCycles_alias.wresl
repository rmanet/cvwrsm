SEQUENCE CYCLE1 {
    model   first
    order   1
    }

SEQUENCE CYCLE2 {
    model   second
    order   2
    }	

SEQUENCE CYCLE3 {
    model   third
    order   3
    }

SEQUENCE CYCLE4 {
    model   fourth
    order   4
    }
	
MODEL first { 

	define sv_1 { value 1 }
	define[local] dv_1 { std kind 'CHANNEL' units 'CFS'}
	define c { value 1 }
}

MODEL second { 

	define[local] sv_2 { value 2 }
	define dv_2_global { lower 400 upper 600 kind 'CHANNEL' units 'CFS'}
}

MODEL third { 

	define[local] as_3 { alias dv_2_global }	
	define[local] int_3 { integer lower 10 upper 20 kind 'integer' units 'CFS'}	
}

MODEL fourth { 

	define[local] alias1 { alias sv_1[first] + dv_2[second] }
	define alias2 { alias as_3[third] + sv_2[not_exist] + sv_2[second] + dv_2_global[second] }
	define alias3 { alias int_3[third] + dv_1[first] }
}
